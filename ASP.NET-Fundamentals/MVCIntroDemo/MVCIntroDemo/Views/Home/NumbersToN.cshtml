@{
    ViewBag.Title = "Nums 1 ... " + ViewBag.Count;
}

<h2>@ViewBag.Title</h2>
<ul>
    @for (int i = 1; i <= ViewBag.Count; i++)
    {
        <li>@i</li>
    }
</ul>

<form asp-controller="Home" asp-action="NumbersToN" method = "post">
    <div class="row">
        <div class="col-2">
            <input class="form-control" type="number" min="0" step="1" name="count"></input>
        </div>
        <div class="col-2">
            <button class="btn-primary">Submit</button>
        </div>
    </div>
</form>